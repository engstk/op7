				<GIC_SPI 687 IRQ_TYPE_LEVEL_HIGH>;

		gfx_0_tbu: gfx_0_tbu@0x2CC5000 {
			compatible = "qcom,qsmmuv500-tbu";
			reg = <0x2CC5000 0x1000>,
				<0x2CC2200 0x8>;
			reg-names = "base", "status-reg";
			qcom,stream-id-range = <0x0 0x400>;
		};

		gfx_1_tbu: gfx_1_tbu@0x2CC9000 {
			compatible = "qcom,qsmmuv500-tbu";
