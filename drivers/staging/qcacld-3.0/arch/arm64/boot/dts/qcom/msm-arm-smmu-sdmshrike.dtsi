&kgsl_smmu {
	qcom,actlr =
		/* All CBs of GFX: +15 deep PF */
		<0x0 0x3ff 0x303>,
		<0x400 0x3ff 0x303>,
		<0x800 0x3ff 0x303>,
		<0xc00 0x3ff 0x303>;
};

&apps_smmu {
	qcom,actlr =
		/* HF0/1, SF TBU +3 PF */
		<0x800 0x3ff 0x103>,
		<0xc00 0x3ff 0x103>,
		<0x2000 0x3ff 0x103>,

		/* NPU SIDs: +15 deep PF */
		<0x1480 0x7 0x303>,
		<0x1080 0x7 0x303>;
};
